   "source": [
    "def palindrome(string):\n",
    "    rev=''\n",
    "    for i in range(len(string)-1,-1,-1):\n",
    "        rev+=(string[i])\n",
    "    if string==rev:\n",
    "        return print(\"the string is palindrome\")\n",
    "    else:\n",
    "        return print(\"this is not palindrome\")"
   ]
